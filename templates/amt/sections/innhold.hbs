{{#if deltaker.innhold}}
    <section>
        <h2>Dette er innholdet</h2>
        {{#if deltaker.innhold.ledetekst}}
            <p>{{deltaker.innhold.ledetekst}}</p>
        {{/if}}
        {{#if deltaker.innhold.fritekstBeskrivelse}}
            <br />
            <p class="fritekst">{{deltaker.innhold.fritekstBeskrivelse}}</p>
        {{/if}}

        {{#if deltaker.innhold.valgteInnholdselementer.length}}
            <ul>
                {{#each deltaker.innhold.valgteInnholdselementer}}
                    <li class="fritekst">{{this}}</li>
                {{/each}}
            </ul>
        {{/if}}
    </section>
{{/if}}